# Environment configuration for NEXUS Live Feed
# Recommended cadences and SLA thresholds (seconds) for each metric
# Copy this file to `.env` and adjust values as needed.

# --- Terrestrial Chronicle ---
SEISMIC_CADENCE=20
SEISMIC_SLA=600

SCHUMANN_CADENCE=1
SCHUMANN_AGGREGATE=10
SCHUMANN_SLA=30

# --- Geomagnetic Chronicle ---
KP_CADENCE=10800
KP_POLL=300
KP_SLA=21600

DST_CADENCE=3600
DST_POLL=300
DST_SLA=7200

MAG_FIELD_CADENCE=86400
MAG_FIELD_POLL=300
MAG_FIELD_SLA=172800

# --- Heliospheric Chronicle ---
SOLAR_WIND_CADENCE=60
SOLAR_WIND_SLA=600

SPACE_WEATHER_CADENCE=300
SPACE_WEATHER_SLA=900

EPHEMERIS_CADENCE=300
EPHEMERIS_ARCHIVE_CADENCE=3600
EPHEMERIS_SLA=900

NEO_CADENCE=86400
NEO_SLA=172800

SAT_PASS_CADENCE=900
SAT_PASS_SLA=1800

GCR_CADENCE=300
GCR_SLA=900

# --- Bio-Resonant Chronicle ---
BIO_RESONANT_CADENCE=2
BIO_RESONANT_SLA=10
